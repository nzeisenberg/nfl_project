   "source": [
    "### Creating new columns\n",
    "cover_list = []\n",
    "cover_margin_list = []\n",
    "for idx in range(len(scores)):\n",
    "    current_score = scores.iloc[idx]\n",
    "    if current_score['winner'] == current_score['favorite']:\n",
